<p id="notice"><%= notice %></p>

<h1>Mslanes</h1>

<table>
  <thead>
    <tr>
      <th>Title</th>
      <th>Category</th>
      <th>Release</th>
      <th>Cost</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @mslanes.each do |mslane| %>
      <tr>
        <td><%= mslane.Title %></td>
        <td><%= mslane.Category %></td>
        <td><%= mslane.Release %></td>
        <td><%= mslane.Cost %></td>
        <td><%= link_to 'Show', mslane %></td>
        <td><%= link_to 'Edit', edit_mslane_path(mslane) %></td>
        <td><%= link_to 'Destroy', mslane, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Mslane', new_mslane_path %>
